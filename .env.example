# Infrastructure Environment Variables
# Copy this file to .env and fill in the values

# GCP Project ID for local PubSub emulator
GCP_PROJECT_ID_LOCAL=miniclue-gcp-local-sr

# PubSub Emulator Host (Docker container)
PUBSUB_EMULATOR_HOST=localhost:8085

# Supabase Local Development
# Note: Most Supabase configuration is in supabase/config.toml
# These are supplementary environment variables for the Supabase CLI
SUPABASE_JWKS_URL=http://127.0.0.1:54321/auth/v1/.well-known/jwks.json

# Google OAuth credentials for Supabase Auth (set in supabase/config.toml via env())
SUPABASE_AUTH_GOOGLE_CLIENT_ID=
SUPABASE_AUTH_GOOGLE_SECRET=
